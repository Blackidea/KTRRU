//- Аргумент - items (массив объектов)
//- items.header - заголовок списка
//- items.item - элемент списка
mixin footer-menu(items)
  +b('div').footer-menu
    each h in items
      +e('ul').list
        +e('li').header
          +e('a')(href='#')
            != h.header
        each item in h.item
          +e('li').item
            +e('a')(href='#')
              != item
